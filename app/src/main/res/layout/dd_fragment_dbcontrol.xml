<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.starnamu.projcet.memorize_card.fragment_folder.DBControlFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/initData"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="initData"
            android:text="데이터 초기화" />

        <Button
            android:id="@+id/lev1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="goLev1"
            android:text="수준 1" />

        <Button
            android:id="@+id/lev2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="goLev2"
            android:text="수준 2" />

        <Button
            android:id="@+id/lev3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="goLev3"
            android:text="수준 3" />

        <Button
            android:id="@+id/upWord1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="upWord1"
            android:text="when 레벨업" />

        <Button
            android:id="@+id/upWord2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="it 레벨업" />

        <Button
            android:id="@+id/upWord3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="sure 레벨업" />
    </LinearLayout>
</FrameLayout>
